{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es5",
        "noImplicitAny": false,
        "experimentalDecorators": true,
        "sourceMap": true,
        "outDir": "./dist",
        "typeRoots": [
            "node_modules/@types"
        ],
        "lib": [
            "es2015",
            "es2016.array.include"
        ]
    },
    "compileOnSave": true,
    "exclude": [
        "dist",
        "node_modules"
    ]
}
